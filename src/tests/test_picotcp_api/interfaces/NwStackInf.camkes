 seos_err_t    read         ( in int handle,in int len );
 seos_err_t    connect      ( in int handle, in string serverName, in int port );
 seos_err_t    accept       ( in int handle,in uint16_t port );
 seos_err_t    listen       ( in int handle,in int backlog );
 seos_err_t    bind         ( in int handle,in uint16_t port );
 void          init         ( void);
   
};