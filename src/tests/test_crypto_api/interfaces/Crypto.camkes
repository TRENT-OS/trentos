procedure CryptoInf {
    include "seos_err.h";
    include "SeosCryptoRpc.h";

    seos_err_t      getRpcHandle    (inout SeosCryptoRpc_Handle handle);
    void            closeRpcHandle  (in SeosCryptoRpc_Handle handle);
};

procedure CryptoRpcInf {
    include "seos_err.h";
    include "SeosCryptoRpc.h";

    seos_err_t      rngGetBytes     (in SeosCryptoRpc_Handle handle, in size_t bufSize);
    seos_err_t      rngReSeed       (in SeosCryptoRpc_Handle handle, in size_t seedLen);
    seos_err_t      digestInit      (in SeosCryptoRpc_Handle handle, out SeosCrypto_DigestHandle digestHandle, in unsigned int algorithm, in size_t ivLen);
    seos_err_t      digestClose     (in SeosCryptoRpc_Handle handle, in SeosCrypto_DigestHandle digestHandle);
    seos_err_t      digestUpdate    (in SeosCryptoRpc_Handle handle, in SeosCrypto_DigestHandle digestHandle, in size_t len);
    seos_err_t      digestFinalize  (in SeosCryptoRpc_Handle handle, in SeosCrypto_DigestHandle digestHandle, in size_t len);
    seos_err_t      keyGenerate     (in SeosCryptoRpc_Handle handle, out SeosCrypto_KeyHandle pKeyHandle, in unsigned int algorithm, in unsigned int flags, in size_t lenBits);
    seos_err_t      keyImport       (in SeosCryptoRpc_Handle handle, out SeosCrypto_KeyHandle pKeyHandle, in unsigned int algorithm, in unsigned int flags, in size_t lenBits);
    seos_err_t      keyClose        (in SeosCryptoRpc_Handle handle, in SeosCrypto_KeyHandle pKeyHandle);
    seos_err_t      cipherInit      (in SeosCryptoRpc_Handle handle, out SeosCrypto_CipherHandle pCipherHandle, in unsigned int algorithm, in SeosCrypto_KeyHandle keyHandle, in size_t ivLen);
    seos_err_t      cipherClose     (in SeosCryptoRpc_Handle handle, in SeosCrypto_CipherHandle pCipherHandle);
    seos_err_t      cipherUpdate    (in SeosCryptoRpc_Handle handle, in SeosCrypto_CipherHandle pCipherHandle, in size_t len);
    seos_err_t      cipherUpdateAd  (in SeosCryptoRpc_Handle handle, in SeosCrypto_CipherHandle pCipherHandle, in size_t len);
    seos_err_t      cipherFinalize  (in SeosCryptoRpc_Handle handle, in SeosCrypto_CipherHandle pCipherHandle);
    seos_err_t      cipherVerifyTag (in SeosCryptoRpc_Handle handle, in SeosCrypto_CipherHandle pCipherHandle, in size_t len);
};

