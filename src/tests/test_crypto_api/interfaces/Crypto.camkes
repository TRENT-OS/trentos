procedure CryptoInf {
    include "seos/seos_err.h";
    include "seos/SeosCryptoRpc.h";

    seos_err_t      getRpcHandle    (inout SeosCryptoRpc_Handle handle);
    void            closeRpcHandle  (in SeosCryptoRpc_Handle handle);
};

procedure CryptoRpcInf {
    include "seos/seos_err.h";
    include "seos/SeosCryptoRpc.h";

    seos_err_t      getRandomData   (in SeosCryptoRpc_Handle handle, unsigned int flags, size_t salt_len, size_t data_len);
    seos_err_t      digestInit      (in SeosCryptoRpc_Handle handle, in SeosCryptoDigest_Algorithm algorithm, in size_t ivLen);
    void            digestClose     (in SeosCryptoRpc_Handle handle);
    seos_err_t      digestUpdate    (in SeosCryptoRpc_Handle handle, in size_t len);
    seos_err_t      digestFinalize  (in SeosCryptoRpc_Handle handle, in size_t len);
    seos_err_t      cipherInit      (in SeosCryptoRpc_Handle handle, in SeosCryptoCipher_Algorithm algorithm, in SeosCrypto_KeyHandle keyHandle, in size_t ivLen);
    void            cipherClose     (in SeosCryptoRpc_Handle handle);
    seos_err_t      cipherUpdate    (in SeosCryptoRpc_Handle handle, in size_t len);
    seos_err_t      cipherFinalize  (in SeosCryptoRpc_Handle handle, in size_t len);
    seos_err_t      keyCreate       (in SeosCryptoRpc_Handle handle, in SeosCryptoCipher_Algorithm algorithm, in unsigned int flags, in size_t lenBits, inout SeosCrypto_KeyHandle pKeyHandle);
};
