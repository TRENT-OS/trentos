    seos_err_t      getKey          (in SeosKeyStoreRpc_Handle handle, out SeosCrypto_KeyHandle keyHandle);
    seos_err_t      deleteKey       (in SeosKeyStoreRpc_Handle handle, in SeosCrypto_KeyHandle keyHandle);
    seos_err_t      closeKey        (in SeosKeyStoreRpc_Handle handle, in SeosCrypto_KeyHandle keyHandle);
