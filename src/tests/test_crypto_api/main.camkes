import <std_connector.camkes>;

import "components/MAIN/MAIN.camkes";
import "components/CRYPTO/CRYPTO.camkes";

assembly {
    composition {
        component MAIN              main;
        component CRYPTO            crypto;

        connection seL4SharedData   cryptoData      (from main.cryptoClientDataport, to crypto.serverDataport);
        connection seL4RPCCall      cryptoRpc       (from main.Crypto, to crypto.Crypto);
        connection seL4RPCCall      seosCryptoRpc   (from main.SeosCryptoRpc, to crypto.SeosCryptoRpc);
    }
    configuration {
    }
}

